{
  "name": "vue-to-reactive",
  "version": "1.0.3",
  "description": "Vue 3 utility converting ref to reactive proxy object",
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist",
    "logo.svg",
    "banner.svg"
  ],
  "scripts": {
    "build": "unbuild",
    "prepublishOnly": "cp README.npm.md README.md && npm run build",
    "postpublish": "cp README.github.md README.md"
  },
  "keywords": [
    "vue",
    "vue3",
    "vue-3",
    "composable",
    "composition-api",
    "vueuse",
    "reactive",
    "reactivity",
    "computed",
    "ref",
    "state",
    "convert",
    "unwrap",
    "proxy",
    "object",
    "no-value",
    "toReactive"
  ],
  "author": "VueFrag",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vuefrag/vue-to-reactive.git"
  },
  "bugs": {
    "url": "https://github.com/vuefrag/vue-to-reactive/issues"
  },
  "homepage": "https://github.com/vuefrag/vue-to-reactive#readme",
  "peerDependencies": {
    "vue": ">=3.0.0"
  },
  "devDependencies": {
    "unbuild": "^2.0.0",
    "typescript": "^5.0.0",
    "vue": "^3.4.0"
  }
}
